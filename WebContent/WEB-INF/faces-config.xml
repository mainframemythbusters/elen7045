<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>customerCredentials</managed-bean-name>
		<managed-bean-class>net.java.aps.model.internal.CustomerCredentials</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customer</managed-bean-name>
		<managed-bean-class>net.java.aps.model.internal.Customer</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerService</managed-bean-name>
		<managed-bean-class>net.java.aps.service.CustomerService</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>profileService</managed-bean-name>
		<managed-bean-class>net.java.aps.service.ProfileService</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>profile</managed-bean-name>
		<managed-bean-class>net.java.aps.model.internal.Profile</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>account</managed-bean-name>
		<managed-bean-class>net.java.aps.model.internal.Account</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>accountCredentials</managed-bean-name>
		<managed-bean-class>net.java.aps.model.internal.AccountCredentials</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>billingCompany</managed-bean-name>
		<managed-bean-class>net.java.aps.model.internal.BillingCompany</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>frontEndBean</managed-bean-name>
		<managed-bean-class>net.java.aps.model.internal.FrontEndBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcome.xhtml</display-name>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/addedaccounts.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
